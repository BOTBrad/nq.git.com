{
	"configVersion": "1",
	"deviceDetected": false,
	"https": false,
	"liveSite": false,
	"locales": ["en"],
	"templateVersion": 3,
	"features": [
		{
			"name": "My Sitemap",
			"sitemap": {
				"urlTemplate": "desktop.urls.xmlSitemap"
			}
		},
		{
			"name": "Locations",
			"locationPageSet": {
				"urlTemplate": "desktop.urls.locationPageSet1",
				"template": "desktop.location.default",
				"outputFormat": ["Html", "Json"],
				"locationScope" : {
					"savedSearch" : "foo"
				}
			}
		},
		{
			"name": "Locations2",
    			"locationPageSet": {
				"urlTemplate": "desktop.urls.locationPageSet2",
      				"template": "desktop.location.default",
      				"outputFormat": [
        				"Html",
        				"Json"
      				],
      				"locationScope": {
		  			"savedSearch": "nq"
      				}
    			}
  		},
		{
			"name": "Autocomplete 1",
			"autocomplete": {
				"urlTemplate": "desktop.urls.autocomplete",
				"locationPageSet": "Locations",
				"fields": [
					{
						"field": "State",
						"param": "state",
						"mode": "EQUALS"
					}
				]
			}
		},
		{
			"name": "Searcher 1",
			"search": {
				"locationPageSet": "Locations",
				"locationScopeOverride" : {
					"savedSearch" : "NY"
				},
				"urlTemplate": "desktop.urls.search1",
				"template": "desktop.search.default",
				"queryParam": "q",
				"maxRadiusParam": {
					"name": "r",
					"default": 50
				},
				"pageResultParam": {
					"name": "per",
					"default": 10
				},
				"pageToShowParam": "p",
				"totalResultParam": {
					"name": "max",
					"default": 50
				},
				"searchFields": [
					{		
 						"field": "State",		
 						"param": "state",		
 						"mode": "EQUALS"		
 					}
				]
			}
		},
		{
			"name": "Searcher 2",
			"search": {
				"locationPageSet": "Locations",
				"locationScopeOverride" : {
					"savedSearch" : "NY"
				},
				"urlTemplate": "desktop.urls.search2",
				"template": "desktop.search.default",
				"queryParam": "q",
				"maxRadiusParam": {
					"name": "r",
					"default": 50
				},
				"pageResultParam": {
					"name": "per",
					"default": 10
				},
				"pageToShowParam": "p",
				"totalResultParam": {
					"name": "max",
					"default": 50
				},
				"searchFields": [
					{		
 						"field": "State",		
 						"param": "state",		
 						"mode": "EQUALS"		
 					}
				]
			}
		},
		{
			"name": "Location Directory 1",
			"locationDirectory": {
				"levels": [
					{
						"urlTemplate": "desktop.urls.directory1State",
						"template": "desktop.directory.stateList",
						"field": "State",
						"orderBy": {
							"field": "State",
							"order": "ascending"
						}
					},
					{
						"urlTemplate": "desktop.urls.directory1Location",
						"template": "desktop.directory.locationList",
						"field": "Locations",
						"orderBy": {
							"field": "Locations",
							"order": "ascending"
						}
					}
				],
				"locationPageSet": "Locations",
				"outputFormat": ["Html", "Json"]
			}
		},
		{
			"name": "Location Directory 2",
    			"locationDirectory": {
      				"levels": [{
          				"urlTemplate": "desktop.urls.directory2State",
          				"template": "desktop.directory.stateList",
          				"field": "nq options",
          				"orderBy": {
            					"field": "nq options",
            					"order": "ascending"
          				}
        			},
        			{
          				"urlTemplate": "desktop.urls.directory2Location",
          				"template": "desktop.directory.locationList",
          				"field": "Locations",
          				"orderBy": {
            					"field": "Locations",
            					"order": "ascending"
          				}
        			}],
      				"locationPageSet": "Locations2",
      				"outputFormat": ["Html", "Json"]
    			}
		},
		{
			"name": "Publishing Pause for Testing",
			"pause_publishing": {
				"time": 60000
			}
		}
	]
}
