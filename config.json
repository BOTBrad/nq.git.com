{
	"configVersion": "1",
	"deviceDetected": false,
	"https": false,
	"liveSite": false,
	"locales": ["en"],
	"templateVersion": 3,
	"locationScope": {
   	  "savedSearch": "NY"
  	},
	"features": [
		{
			"name": "Locations",
			"locationPageSet": {
				"urlTemplate": "url.location",
				"template": "desktop.location.default",
				"outputFormat": ["Html", "Json"],
				"locationScope": {
   	  				"savedSearch": "NY"
  				},
			}
		},
		{
			"name": "Location Directory 1",
			"locationDirectory": {
				"levels": [
					{
						"urlTemplate": "url.directoryStateList",
						"template": "desktop.directory.stateList",
						"field": "State",
						"orderBy": {
							"field": "State",
							"order": "ascending"
						}
					},
					{
						"urlTemplate": "url.directoryCityList",
						"template": "desktop.directory.cityList",
						"field": "City",
						"orderBy": {
							"field": "City",
							"order": "ascending"
						}
					},
          				{
            					"urlTemplate": "url.directoryLocationList",
           					 "template": "desktop.directory.locationList",
            					"field": "Locations",
            					"orderBy": {
              						"field": "Locations",
              						"order": "ascending"
            					}
					}
				],
				"locationPageSet": "Locations",
				"outputFormat": ["Html"]
			}
		},
		{
			"name": "Publishing Pause for Testing",
			"pause_publishing": {
				"time": 60000
			}
		}
	]
}
